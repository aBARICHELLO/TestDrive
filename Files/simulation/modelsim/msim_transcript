add wave  \
sim:/reg_in/SET_ROL \
sim:/reg_in/EN_TIME \
sim:/reg_in/CLOCK_M \
sim:/reg_in/RST \
sim:/reg_in/REG_IN \
sim:/reg_in/SPEED \
sim:/reg_in/REG_OUT_EXIT \
sim:/reg_in/enable \
sim:/reg_in/signalshift
